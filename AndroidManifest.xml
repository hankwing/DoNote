        </service>
        <service
            android:name="com.baidu.location.f"
            android:enabled="true"
            android:process=":remote" >

            <!--
          		<intent-filter>
				<action android:name="com.baidu.location.service_v2.9"></action>
			</intent-filter>
            -->
        </service>
        <service  
            android:name="com.donote.util.MyShakeBootService"
            android:enabled="true" >
        </service>
        
        <meta-data android:name="BaiduMobAd_CHANNEL" android:value="Baidu Market" />
		<!-- 您从百度网站获取的APP KEY -->
		<meta-data android:name="BaiduMobAd_STAT_ID" android:value="164d6a5f61" />
		<!-- 是否开启错误日志统计，默认为false -->
		<meta-data android:name="BaiduMobAd_EXCEPTION_LOG" android:value="false" />
		<!-- 日志发送策略，可选值：APP_START、ONCE_A_DAY、SET_TIME_INTERVAL，默认为APP_START -->
		<meta-data android:name="BaiduMobAd_SEND_STRATEGY" android:value="APP_START" />
		<!-- 日志发送策略 为SET_TIME_INTERVAL时，需设定时间间隔。取值为1-24的整数，默认为1 -->
		<!-- <meta-data android:name="BaiduMobAd_TIME_INTERVAL" android:value="2" /> -->
		<!-- 日志仅在wifi网络下发送，默认为false -->
		<meta-data android:name="BaiduMobAd_ONLY_WIFI" android:value="false" />
        <receiver 
            android:name="com.donote.widget.DoNoteWidgetProvider"
            android:exported="true">
            <intent-filter >
                <action android:name="com.donote.action.widget.click"/>
                <action android:name="com.donote.action.widget.next"/>
                <action android:name="com.donote.action.widget.prior"/>
                <action android:name="android.appwidget.action.APPWIDGET_UPDATE"/>
                <action android:name="android.appwidget.action.APPWIDGET_ENABLED"/>
            </intent-filter>
            <meta-data 
                android:name="android.appwidget.provider"
                android:resource="@xml/donotewidget"/>
        </receiver>
        
        <activity
            android:name="com.donote.activity.WidgetActivity"
            android:launchMode="singleTop" >
        </activity>
