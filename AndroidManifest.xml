        </activity>
        <activity android:name="com.donote.activity.TableEdit" >
        </activity>
        <activity
            android:name="com.donote.freestyle.GraphText"
            android:theme="@style/myDialogTheme" >
        </activity>
        <activity
            android:name="com.donote.freestyle.DefineShape"
            android:screenOrientation="portrait"
            android:theme="@style/myDialogTheme" >
        </activity>
        <activity android:name="com.donote.activity.MainBackground" >
        </activity>
        <activity android:name="com.tencent.weibo.sdk.android.component.Authorize" >
        </activity>
        <activity
            android:name="com.donote.activity.TableText"
            android:theme="@style/myDialogTheme" >
        </activity>
        <activity
            android:name="com.donote.freestyle.DefineMenu"
            android:theme="@style/myDialogTheme"
            android:windowSoftInputMode="adjustPan" >
        </activity>
        <activity
            android:name="com.donote.activity.CommonEdit"
            android:launchMode="singleTask"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="adjustPan" >
        </activity>
        <activity
            android:name="com.donote.activity.DisplayContent"
            android:launchMode="singleTask"
            android:screenOrientation="portrait" >
        </activity>
        <activity
